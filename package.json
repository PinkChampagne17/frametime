{
  "name": "frametime",
  "version": "0.1.0",
  "description": "A package to monitor frametime in browser.",
  "keywords": [
    "frametime",
    "fps"
  ],
  "scripts": {
    "build": "rimraf ./dist && rollup --config"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PinkChampagne17/frametime.git"
  },
  "author": "pinkchampagne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PinkChampagne17/frametime/issues"
  },
  "homepage": "https://github.com/PinkChampagne17/frametime#readme",
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "rimraf": "^4.3.0",
    "rollup": "^3.18.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
